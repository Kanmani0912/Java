import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner obj = new Scanner(System.in);
      int A = obj.nextInt();
      int B = obj.nextInt();
      System.out.println(A+B);
      System.out.println(A-B);
      System.out.println(A*B);
      System.out.println(A/B);
      System.out.println(A%B);
  }
}
